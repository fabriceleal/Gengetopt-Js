{
	"package": "jed",
	"version": "0.01",
	"purpose": "The JSON stream editor",
	"usage" : "jed.js ....", 
	"description" : "The JSON stream editor",

	"args" : "--To --the --command --line --processor --itself",
	
	"data" : [
		{ 
			"tag" : "text" ,
			"value" : "arbitrary text, with line breaks if you want ..."
		},

		{ 
			"tag" : "section" ,
			"name" : "new section",
			"desc" : "description of the new section"
		},

		{ 
			"tag" : "option",
			"..." : "..."
		},

		{
			"tag" : "option-flag",
			"long" : "asd",
			"short" : "a",
			"desc" : "description of a, asd",
			"default" : "on" 
		}
	]
}
